{
  "expo": {
    "name": "ameen",
    "slug": "ameen",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/logo_App.png",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "splash": {
      "image": "./app/assets/logo_App.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "n"
    },
    "android": {
      "enableDangerousExperimentalLeanBuilds": false,
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/logo_App.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.n"
    },
    "web": {
      "favicon": "./assets/logo_App.png"
    },
    "plugins": ["expo-sqlite", "expo-secure-store"],
    "extra": {
      "eas": {
        "projectId": "528b5014-f12c-422d-84d6-f1a1cb3309d3"
      }
    },
    "owner": "futun and muzn"
  }
}
